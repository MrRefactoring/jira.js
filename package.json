{
  "name": "jira.js",
  "version": "1.0.2",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "repository": "https://github.com/MrRefactoring/jira.js.git",
  "author": "Vladislav Tupikin <mrrefactoring@yandex.ru>",
  "license": "MIT",
  "keywords": [
    "jira",
    "api",
    "wrapper",
    "client",
    "rest"
  ],
  "scripts": {
    "compile": "tsc",
    "prepublishOnly": "npm run lint && npm run test && npm run compile",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "doc": "typedoc --out docs --mode file --ignoreCompilerErrors ./src",
    "test": "jest --verbose",
    "test:coverage": "npm run test -- --coverage"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "sinon": "^8.0.4",
    "ts-jest": "^24.3.0",
    "typedoc": "^0.15.7",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "atlassian-jwt": "https://github.com/MrRefactoring/atlassian-jwt.git#dist",
    "axios": "^0.19.1"
  }
}
