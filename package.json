  "author": "Vladislav Tupikin <mrrefactoring@yandex.ru>",
  "license": "MIT",
  "keywords": [
    "jira",
    "api",
    "wrapper",
    "client",
    "rest"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build && npm run test && npm run lint",
